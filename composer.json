{
    "name": "kgcoder/laravel-ar-dompdf",
    "description": "AN AR DOMPDF Wrapper for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "ar-dompdf","ar", "pdf"],
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        },
        {
            "name": "Khaled Gamal",
            "email": "khaledgamalsabbah@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|6.x|7.x|8.x",
        "kgcoder/ar-dompdf": "^1.0"
    },

    "autoload": {
        "psr-4": {
            "Kgcoder\\DomPDF\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        },
        "laravel": {
            "providers": [
                "Kgcoder\\DomPDF\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "Kgcoder\\DomPDF\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
